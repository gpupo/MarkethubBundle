{
    "name": "gpupo/markethub-bundle",
    "description": "Integração de SDKs para Marketplaces Brasileiros",
    "keywords": ["Marketplace", "B2W", "Via Varejo", "Cnova", "Sky Hub", "Netshoes", "Mercadolivre", "Symfony", "SDK"],
    "homepage": "https://opensource.gpupo.com/MarkethubBundle/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gpupo\\MarkethubBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\MarkethubBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/http-kernel": "^6.2",
        "symfony/dependency-injection": "^6.4",
        "symfony/config": "^6.4",
        "symfony/monolog-bundle": "^3.0",
        "gpupo/mercadolivre-sdk": "^6.1 || dev-main",
        "gpupo/mercadopago-sdk": "^6.1|| dev-main",
        "gpupo/submarino-sdk": "^6.1 || dev-main",
        "gpupo/shipping-services":"^3.1 || dev-main",
        "theseer/tokenizer": "^1.1",
        "gpupo/common-schema": "^6.3 || dev-main"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    },
    "scripts": {
        "test": "simple-phpunit"
    },
    "suggest": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
      "symfony": {
        "allow-contrib": false,
        "require": "6.4"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
